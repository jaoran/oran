n\ttext-shadow: 3px 3px 5px #000;\r\n}\r\n#myheader h2 {\r\n\tposition: relative;\r\n\ttop: 65px;\r\n\ttext-align: center;\r\n\tfont-weight: 100;\r\n\tcolor: #fff;\r\n\tfont-size: 35px;\r\n\tletter-spacing: 4px;\r\n\ttext-shadow: 3px 3px 5px #000;\r\n\tfont-family: 'Poiret One', cursive;\r\n}\r\n#container {\r\nmax-width: 1100px;\r\n\tmargin: 0 auto;\r\n}\r\n#content {\r\n\tpadding-bottom: 50px;\r\n}\r\n#content h2 {\r\n\tfont-family: 'Dancing Script', cursive;\r\n\tpadding-top: 30px;\r\n\tpadding-bottom: 10px;\r\n\tmargin-bottom: 20px;\r\n\tfont-size: 26px;\r\n\tfont-weight: 100;\r\n\tcolor: #222;\r\n\ttext-align: center;\r\n\tborder-bottom-width: 2px;\r\n\tborder-bottom-style: solid;\r\n\tborder-bottom-color: rgba(255,217,104,1.00);\r\n}\r\n#content .article_1 {\r\n\tcolumn-count: 3;\r\n\tcolumn-gap: 20px;\r\n}\r\n#content .article_1 p {\r\n\tfont-family: 'Gloria Hallelujah', cursive;\r\n\tcolor: #333;\r\n\tfont-size: 17px;\r\n\tfont-weight: 100;\r\n\tpadding: 0 0 10px 0;\r\n\tline-height: 21px !important;\r\n}\r\n#content .article_1 p::first-letter {\r\n\tfont-size: 26px;\r\n\tfont-family: 'Cinzel', serif;\r\n\tfont-weight: 200;\r\n}\r\n#content .article_1 p img.article_img {\r\n\tfloat: right;\r\n\tmargin: 4px;\r\n}\r\nfooter {\r\n\tbackground-color: #000;\r\n\tpadding: 20px 0 120px 0;\r\n}\r\nfooter h2 {\r\n\ttext-align: center;\r\n\tfont-weight: 100;\r\n\tcolor: #fff;\r\n\tletter-spacing: 4px;\r\n\tfont-family: 'Poiret One', cursive;\r\n\tpadding-bottom: 10px;\r\n\tmargin: 0 auto;\r\n\tmargin-bottom: 20px;\r\n\tfont-size: 26px;\r\n\tborder-bottom-width: 2px;\r\n\tborder-bottom-style: solid;\r\n\tborder-bottom-color: rgba(255,217,104,1.00);\r\n\twidth: 950px;\r\n}\r\nfooter article {\r\n\twidth: 960px;\r\n\tdisplay: block;\r\n\tmargin: 0 auto;\r\n\ttext-align: center;\r\n\tcolumn-count: 4;\r\n\tcolumn-gap: 40px;\r\n\tcolor: rgba(255,217,104,1.00);\r\n}\r\n\r\n/* tablets (landscape) ----------- */\r\n@media only screen and (min-width : 700px) and (max-width : 1100px) and (orientation : landscape) {\r\n#mainnav #nav_title {\r\n\theight: 65px;\r\n}\r\n#myheader {\r\n\tmin-height: 210px;\r\n\tbackground-size: contain;\r\n}\r\n#myheader h1 {\r\n\tfont-size: 30px;\r\n}\r\n#myheader h2 {\r\n\tfont-size: 20px;\r\n}\r\n#container {\r\n\twidth: 95%;\r\n}\r\n#content {\r\n\tpadding-bottom: 20px;\r\n}\r\n#content h2 {\r\n\tfont-size: 20px;\r\n}\r\n#content .article_1 {\r\n\tcolumn-count: 2;\r\n\tcolumn-gap: 10px;\r\n}\r\n#content .article_1 p {\r\n\tfont-size: 16px;\r\n\tline-height: 18px !important;\r\n}\r\n#content .article_1 p::first-letter {\r\n\tfont-size: 19px;\r\n}\r\n#content .article_1 p img.article_img {\r\n\tmargin: 2px;\r\n}\r\nfooter {\r\n\tpadding: 20px 0 60px 0;\r\n}\r\nfooter h2 {\r\n\tfont-size: 20px;\r\n\twidth: 95%;\r\n}\r\nfooter article {\r\n\twidth: 95%;\r\n\tcolumn-count: 3;\r\n\tcolumn-gap: 20px;\r\n}\r\n}\r\n\r\n/* iPads (portrait) ----------- */\r\n@media only screen and (min-width : 700px) and (max-width : 1100px) and (orientation : portrait) {\r\n/* Styles */\r\n#mainnav #nav_title {\r\n\theight: 65px;\r\n}\r\n#myheader {\r\n\tmin-height: 210px;\r\n\tbackground-size: contain;\r\n}\r\n#myheader h1 {\r\n\tfont-size: 30px;\r\n}\r\n#myheader h2 {\r\n\tfont-size: 20px;\r\n}\r\n#container {\r\n\twidth: 95%;\r\n}\r\n#content {\r\n\tpadding-bottom: 20px;\r\n}\r\n#content h2 {\r\n\tfont-size: 20px;\r\n}\r\n#content .article_1 {\r\n\tcolumn-count: 2;\r\n\tcolumn-gap: 10px;\r\n}\r\n#content .article_1 p {\r\n\tfont-size: 16px;\r\n\tline-height: 18px !important;\r\n}\r\n#content .article_1 p::first-letter {\r\n\tfont-size: 19px;\r\n}\r\n#content .article_1 p img.article_img {\r\n\tmargin: 2px;\r\n}\r\nfooter {\r\n\tpadding: 20px 0 60px 0;\r\n}\r\nfooter h2 {\r\n\tfont-size: 20px;\r\n\twidth: 95%;\r\n}\r\nfooter article {\r\n\twidth: 95%;\r\n\tcolumn-count: 3;\r\n\tcolumn-gap: 20px;\r\n}\r\n}\r\n\r\n/* Smartphones (portrait and landscape) ----------- */\r\n@media only screen and (max-width : 699px) {\r\n/* Styles */\r\n\r\n.sb_showing #menu-trigger {\r\n\tleft: 245px;\r\n}\r\n#mainnav {\r\n}\r\n.sb_hidden #mainnav {\r\n}\r\n.sb_showing #mainnav {\r\n}\r\n#mainnav #nav_title {\r\n\theight: 65px;\r\n\ttext-align: left;\r\n}\r\n#myheader {\r\n\tmin-height: 210px;\r\n\tbackground-size: contain;\r\n}\r\n#myheader h1 {\r\n\tfont-size: 30px;\r\n}\r\n#myheader h2 {\r\n\tfont-size: 22px;\r\n}\r\n#container {\r\n\twidth: 95%;\r\n}\r\n#content {\r\n\tpadding-bottom: 20px;\r\n}\r\n#content h2 {\r\n\tfont-size: 22px;\r\n}\r\n#content .article_1 {\r\n\tcolumn-count: 1;\r\n\tcolumn-gap: 10px;\r\n}\r\n#content .article_1 p {\r\n\tfont-size: 18px;\r\n\tline-height: 20px !important;\r\n}\r\n#content .article_1 p::first-letter {\r\n\tfont-size: 19px;\r\n}\r\n#content .article_1 p img.article_img {\r\n\tmargin: 2px;\r\n}\r\nfooter {\r\n\tpadding: 20px 0 60px 0;\r\n}\r\nfooter h2 {\r\n\tfont-size: 20px;\r\n\twidth: 95%;\r\n}\r\nfooter article {\r\n\twidth: 95%;\r\n\tcolumn-count: 2;\r\n\tcolumn-gap: 10px;\r\n}\r\n}/*end media query*/\r\n
