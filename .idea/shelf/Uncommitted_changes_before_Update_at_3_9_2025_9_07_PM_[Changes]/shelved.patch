<+>@charset \"UTF-8\";\r\n/* CSS Document */\r\n\r\n@import url('https://fonts.googleapis.com/css2?family=Dancing+Script:wght@400;600&family=Gloria+Hallelujah&display=swap');\r\n/*\r\nfont-family: 'Cinzel', serif;\r\nfont-family: 'Poiret One', cursive;\r\n*/\r\n\r\n/*resets*/\r\n* {\r\n\tpadding: 0;\r\n\tmargin: 0;\r\n\tbox-sizing: border-box;\r\n\t-webkit-overflow-scrolling: touch;\r\n}\r\n.myclear {\r\n\tclear: both;\r\n}\r\nbody {\r\n\tfont-family: 'Cinzel', serif;\r\n\tbackground-color: #919191;\r\n}\r\n#mega-wrapper {\r\n\tposition: fixed;\r\n\ttop: 0;\r\n\tright: 0;\r\n\twidth: 100%;\r\n\tmin-height: 100%;\r\n}\r\n/*moving parts********************************************************************************/\r\n#sidebar {\r\n\r\n\twidth: 300px;\r\n\theight: 100%;\r\n\toverflow-x: hidden;\r\n\toverflow-y: hidden;\r\n\tz-index: 3;\r\n\tperspective:100px;\r\n\ttransition: transform 1.5s ease-in-out;\r\n\ttransform: translateX(10px);\r\n}\r\n.sb_hidden #sidebar {\r\n\ttransform: translate3d(-300px, 0, 0);\r\n}\r\n.sb_showing #sidebar {\r\n\ttransform: translate3d(0px, 0, 0);\r\n}\r\n#page {\r\n\twidth: 100vw;\r\n\tposition: fixed;\r\n\ttop: 0;\r\n\tleft: 0;\r\n\toverflow-x: hidden;\r\n\toverflow-y: scroll;\r\n\tbackground-color: #fff;\r\n\theight: 100vh;\r\n\tz-index: 3;\r\ntransition: transform 0.5s ease-in-out;\r\n}\r\n.sb_hidden #page {\r\n\ttransform: translate3d(0px, 0, 0);\r\n}\r\n.sb_showing #page {\r\n\ttransform: translate3d(300px, 0, 0) ;\r\n}\r\n/*the menu button itself*/\r\n#menu-trigger {\r\n\tposition: absolute;\r\n\ttop: 3px;\r\n\tleft: 0;\r\n\twidth: 53px;\r\n\theight: 50px;\r\n\tcursor: pointer;\r\n\tbackground-image: url(../sidebar_images/menu_iconS.png);\r\n\tbackground-repeat: no-repeat;\r\n\tz-index: 999;\r\n\ttransition: right 1.5s ease-in-out, transform 1.5s ease-in-out;\r\n}\r\n.sb_hidden #menu-trigger {\r\n\tright: 0;\r\n\ttransform: rotate(180deg);\r\n}\r\n.sb_showing #menu-trigger {\r\n\tright: 303px;\r\n\ttransform: rotate(-90deg);\r\n}